<odoo>
        <report id="report_tienda_lineas" string="Reporte personalizado" model="agenda.contactos" report_type="qweb-pdf" name="agenda.report"
             file="agenda.report"/>

        <template id="report  agenda">
          <t t-call="web.html_container">
              <t t-foreach="docs" t-as="o">
                  <t t-call="web.external_layout">
                      <div class="page">
                          <h2>Agenda</h2>
                          <table class="egt" border='1' cellspacing='0' cellpadding='2' bordercolor="666633">
                              <tr>
                                <th>DNI</th>
                                <th>Nombre</th>
                                  <th>Provincia</th>
                              </tr>
                              <tr>
                                  <td><div t-field="o.dni"/></td>
                                  <td><div t-field="o.nombre"/></td>
                                  <td><div t-field="o.provincia"/></td>
                                </tr>
                        </table>
                      </div>
                  </t>
              </t>
          </t>
        </template>

</odoo>

